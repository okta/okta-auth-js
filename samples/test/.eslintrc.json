{
  "extends": [
    "eslint:recommended",
    "plugin:jasmine/recommended"
  ],
  "globals": {
    "$": "readonly",
    "browser": "readonly",
    "process": "readonly",
    "__dirname": "readonly"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "plugins": ["jasmine"],
  "env": {
    "jasmine": true
  },
  "rules": {
    "jsdoc/newline-after-description": 0,
    "jsdoc/check-types": 0,
    "jsdoc/valid-types": 0,
    "jsdoc/check-alignment": 0,
    "node/no-unsupported-features/es-syntax": 0,
    "jasmine/no-suite-dupes": [1, "branch"]
  }
}
