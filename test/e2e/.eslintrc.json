{
  "extends": [
    "eslint:recommended",
    "plugin:jasmine/recommended"
  ],
  "globals": {
    "$": "readonly",
    "browser": "readonly",
    "process": "readonly",
    "__dirname": "readonly"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "plugins": ["jasmine"],
  "env": {
    "jasmine": true
  },
  "rules": {
    "jasmine/no-suite-dupes": [1, "branch"]
  }
}
