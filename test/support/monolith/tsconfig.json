{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "rootDir": "../",
    "baseUrl": "../",
    "lib": [
      "ES2020.Promise"
    ],
    "types":[ "node" ]
  }
}